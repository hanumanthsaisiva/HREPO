body { margin: 0px; }

@media (max-width: 350px) {
  div#commentsHolder { padding: 8px 12px 4px; }
  div.dropdown { display: block; margin: 0px; }
  div.dropdown select#identityMenu { width: 100%; }
  span#signoutLink { margin-bottom: 7px; }
}

@media (max-width: 420px) {
  .dialog { left: 10px; right: 10px; width: inherit; }
  .dialog .group { display: block; }
  .dialogField { width: 100%; }
}

@media (min-width: 420px) {
  .dialog { width: 307px; }
  .dialogField { width: 280px; }
}

a, body, div, input, p, span, td { font-family: arial, sans-serif; font-size: 13px; }

a { color: rgb(0, 158, 184); text-decoration: underline; }

input[type="button"] { background-color: rgb(245, 245, 245); border: 1px solid rgba(0, 0, 0, 0.1); border-radius: 2px; color: rgb(34, 34, 34); font-size: 11px; font-weight: bold; height: 27px; margin: 1px 0px 0px 15px; padding-bottom: 0px; text-align: center; text-shadow: rgba(0, 0, 0, 0.1) 0px 1px; vertical-align: top; width: 75px; appearance: none; }

input[type="button"]:active, input[type="button"]:focus:active { border: 1px solid rgb(77, 144, 254); box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 2px inset; }

h3 { margin: 5px 0px; }

label { font-weight: bold; }

.postCommentButtonHolder { background-color: rgb(245, 245, 245); padding: 10px 10px 4px; clear: both; }

.dialog { background: rgb(255, 255, 255); border: 1px solid rgb(238, 238, 238); color: rgb(0, 0, 0); padding: 10px 10px 10px 14px; position: absolute; z-index: 1; border-radius: 6px; box-shadow: rgba(0, 0, 0, 0.2) 1px 1px 1px; }

.dialog h3 { margin: 0px 0px 10px; }

.dialog h4 { margin: 2px 0px; }

.dialog img { margin: 2px 0px; }

.dialog input { margin: 2px 0px; position: relative; }

.dialog .group { margin: 5px 0px; }

.dialog .top h3 { font-size: 16px; font-weight: normal; }

.dialog .errorMsg { padding: 5px 10px; background: rgb(255, 153, 153); color: rgb(102, 0, 0); }

img.close { position: relative; margin: 0px; float: right; cursor: pointer; }

img#captcha-img { border: 1px solid rgb(221, 221, 221); padding: 5px 30px; background: none 0px center repeat scroll rgb(255, 255, 255); }

.shown-openid-url { color: rgb(136, 136, 136); font-style: italic; }

#openIdUriDisplayUsername { color: rgb(0, 0, 0); }

#postCommentSubmit { background-color: rgb(77, 144, 240); border: 1px solid rgb(48, 121, 237); color: rgb(255, 255, 255); font-weight: bold; margin: 0px 8px 6px 0px; }

#postCommentPreview { color: rgb(68, 68, 68); margin: 0px 8px 6px 0px; }

#postCommentSubmit:hover { background-color: rgb(53, 122, 232); background-image: -webkit-linear-gradient(top, rgb(77, 144, 254), rgb(53, 122, 232)); border-color: rgb(47, 91, 183); }

#postCommentPreview:hover, .signoutButton:hover { background-color: rgb(248, 248, 248); background-image: -webkit-linear-gradient(top, rgb(248, 248, 248), rgb(241, 241, 241)); border: 1px solid rgb(198, 198, 198); color: rgb(34, 34, 34); transition: all 0s ease 0s; box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 1px; }

